@FindBy(xpath="/html[1]/body[1]/div[2]/div[2]/div[1]/ul[1]/li[25]/div[1]/div[2]/a[1]")
WebElement qe;


//li[@class='srp clearfix']/div[1]/div[@class='agent_logo']/a[contains(text(),'Â£475,000')]


	
	
	
public void fs() throws Throwable
{
	Thread.sleep(4000);
	qwrt.sendKeys("London");
	tyuio.click();
	
	
	
	
}
	


public void scrollDown() {
	JavascriptExecutor jse = (JavascriptExecutor) driver;
	jse.executeScript("window.scrollBy(0,500)", "");
}


public void cl()
{
	qe.click();
}


public ArrayList<WebElement> ad()
{
	ArrayList<WebElement> qe= new ArrayList<WebElement>();
        Iterator<WebElement> we= qe.iterator();
       int df= qe.size();
       System.out.println(df);
       for(int i=0;i<=df;i++)
        while(we.hasNext())
        {
        	WebElement qr= we.next();
        	System.out.println(qr);
        }


        
        return qe;
}








}


